<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="PlanificacionRentaAC" transaction-type="JTA">
    <jta-data-source>java:app/Heroku</jta-data-source>
    <class>com.uce.entidades.Color</class>
    <class>com.uce.entidades.Marcas</class>
    <class>com.uce.entidades.Ticket</class>
    <class>com.uce.entidades.TipoCombustible</class>
    <class>com.uce.entidades.TiposPagos</class>
    <class>com.uce.entidades.TiposUsuario</class>
    <class>com.uce.entidades.TipoVehiculo</class>
    <class>com.uce.entidades.Usuario</class>
    <class>com.uce.entidades.Vehiculos</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.schema-generation.database.action" value="create"/>
    </properties>
  </persistence-unit>
</persistence>
